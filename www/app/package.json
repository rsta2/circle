{
  "name": "@zx-ncoderz/hd-speccys-app",
  "version": "0.0.0",
  "description": "hd-speccys onboard web / mobile application",
  "author": "Richard Sewell",
  "license": "UNLICENSED",
  "private": true,
  "scripts": {
    "start": "vite",
    "build": "tsc -p tsconfig.json --noEmit && vite build",
    "doc": "typedoc",
    "test": "jest --coverage",
    "lint": "eslint --max-warnings 0 -- './**/*.@(tsx|ts|jsx|js|mjs|cjs)'",
    "clean": "tsc --build --clean && shx rm -rf ./dist",
    "serve": "run http-server ./dist",
    "capacitor": "run cap",
    "start-android": "run build && run capacitor sync && run capacitor run android",
    "start-ios": "run build && run capacitor sync && run capacitor run ios",
    "open-android": "run build && run capacitor sync && run capacitor open android",
    "open-ios": "run build && run capacitor sync && run capacitor open ios"
  },
  "packageManager": "yarn@3.2.4",
  "engines": {
    "node": ">=10",
    "npm": ">=6.8.0"
  },
  "keywords": [],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zx-ncoderz/hd-speccys.git"
  },
  "dependencies": {
    "@ncoderz/env": "^0.0.1",
    "@ncoderz/superlog": "^0.0.2",
    "fast-memoize": "^2.5.2",
    "normalize.css": "^8.0.1",
    "three": "^0.147.0"
  },
  "devDependencies": {
    "@capacitor/android": "^4.6.1",
    "@capacitor/cli": "^4.6.1",
    "@capacitor/core": "^4.6.1",
    "@capacitor/ios": "^4.6.1",
    "@swc/cli": "^0.1.57",
    "@swc/core": "^1.3.14",
    "@types/eslint-plugin-prettier": "^3.1.0",
    "@types/fpsmeter": "^0.3.31",
    "@types/jest": "^29.2.2",
    "@types/node": "^18.11.9",
    "@types/three": "^0.141.0",
    "@typescript-eslint/eslint-plugin": "^5.42.1",
    "@typescript-eslint/parser": "^5.42.1",
    "@vitejs/plugin-basic-ssl": "^1.0.1",
    "capacitor-suppress-longpress-gesture": "^0.0.5",
    "core-js": "^3.11.1",
    "esbuild": "^0.15.13",
    "esbuild-runner": "^2.2.2",
    "eslint": "^8.27.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-json": "^3.1.0",
    "eslint-plugin-prettier": "^4.2.1",
    "esr": "^0.10.0",
    "fpsmeter": "^0.3.1",
    "http-server": "^14.1.1",
    "imagetracerjs": "^1.2.6",
    "jest": "^29.3.0",
    "jest-environment-node": "^29.3.0",
    "jest-junit": "^14.0.1",
    "prettier": "^2.7.1",
    "shx": "^0.3.4",
    "terser": "^5.15.1",
    "ts-jest": "^29.0.3",
    "typedoc": "^0.23.20",
    "typedoc-plugin-markdown": "^3.13.6",
    "typescript": "^4.8.4",
    "url-parse": "^1.5.10",
    "vite": "^3.2.3",
    "vite-tsconfig-paths": "^3.5.2"
  }
}
